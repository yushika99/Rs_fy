<?xml version="1.0" encoding="utf-8"?>
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context=".DailyIncomeAnalyticsActivity"
        android:background="@drawable/incomedis2">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <androidx.appcompat.widget.Toolbar
                android:id="@+id/todayIncomeToolbar"
                android:layout_width="match_parent"
                android:layout_height="68dp"
                android:background="#0759EAB7"
                android:elevation="4dp"
                android:maxWidth="1dp"
                android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
                app:popupTheme="@style/ThemeOverlay.AppCompat.Light"
                tools:targetApi="lollipop" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Daily Income "
                android:textColor="#F5FBFFFE"
                android:textAlignment="center"
                android:textSize="25sp"
                android:textStyle="bold"
                android:shadowColor="#B60558"
                android:shadowDx="35"
                android:shadowDy="67"
                android:shadowRadius="80"
                android:gravity="center_horizontal" />



            <TextView
                android:id="@+id/todaytotalAmoutincome"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Today Total: Rs.0 "
                android:layout_marginTop="7dp"
                android:background="@drawable/headtextsub"
                android:layout_marginHorizontal="80dp"
                android:textSize="18dp"
                android:gravity="center"
                android:textColor="#FFF"
                android:textStyle="bold"/>



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="10dp"
                android:layout_marginRight="20dp"

                android:layout_marginLeft="20dp"
                android:layout_marginBottom="20dp"
                android:background="#FFFFFFFF">

                <ImageView
                    android:layout_width="1dp"
                    android:layout_height="18dp"
                    android:layout_weight="1"
                    android:layout_marginTop="5dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="5dp"
                    android:src="@drawable/green" />
                <TextView
                    android:layout_width="1dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:text="L < 50%"
                    android:textStyle="bold"
                    android:textSize="15dp"
                    android:layout_gravity="center_vertical"
                    android:textColor="#333"/>

                <ImageView
                    android:layout_width="1dp"
                    android:layout_height="18dp"
                    android:layout_weight="1"
                    android:layout_marginTop="5dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="5dp"
                    android:src="@drawable/blue" />
                <TextView
                    android:layout_width="1dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:text="50% - 99% "
                    android:textStyle="bold"
                    android:textSize="15sp"
                    android:layout_gravity="center_vertical"
                    android:textColor="#333"/>

                <ImageView
                    android:layout_width="1dp"
                    android:layout_height="18dp"
                    android:layout_weight="1"
                    android:layout_marginTop="5dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="5dp"
                    android:src="@drawable/red" />
                <TextView
                    android:layout_width="1dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:text=" G > 100% "
                    android:textStyle="bold"
                    android:textSize="12sp"
                    android:layout_gravity="center_vertical"
                    android:textColor="#333"/>

            </LinearLayout>

